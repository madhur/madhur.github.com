---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Madhur's Blog</title>
 <link href="{{ site.blogurl }}/atom.xml" rel="self"/>
 <link href="{{ site.blogurl }}/blogindex.html/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.blogurl }}/blogindex.html</id>
 <author>
   <name>Madhur Ahuja</name>
   <email>ahuja.madhur@gmail.com</email>
 </author>
 
 {% for post in site.posts limit:15 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ site.blogurl }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>